class Solution(object):
    def isPalindrome(self, x):
        if x < 0:
            return False
        
        if(x!=0 and x%10 == 0):
            return False
        
        rev=0
        temp=x
        while(x > 0):
            rem=x%10
            rev= rev * 10 +rem
            x=x/10
        if temp==rev:
            return True
        else:
            return False
        
        
